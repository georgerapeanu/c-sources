#include <iostream>
using namespace std;
int n,k,i,v[100010],j,nr,dp[100010][11],p;
int main()
{
    cin>>n>>k;
    for(i=1;i<=n;i++)
    {cin>>v[i];dp[i][0]=1;}
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=k+1;j++)
        {
            for(p=1;p<i;p++)
            {
                if(v[p]<v[i])
                {
                    dp[v[i]][j]+=dp[k][j-1];
                }
            }
        }
    }
    cout<<dp[n][k+1];
    return 0;
}
