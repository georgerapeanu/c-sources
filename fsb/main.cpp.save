#include <cstdio>
#include <map>
using namespace std;
FILE *f=fopen("fsb.in","r");
FILE *g=fopen("fsb.out","w");
int N,V[100005],i,j,nr;
map <int,int> M;
int main()
{
    fscanf(f,"%d",&N);
    for(i=1;i<=N;i++)
        {
            fscanf(f,"%d",&V[i]);
            if(!V[i])
                V[i]=V[i-1]-1;
            else
                V[i]=V[i-1]+1;
            
            nr+=M[V[i]];
            M[V[i]]++;
        }
    fprintf(g,"%d",nr);
    fclose(f);
    fclose(g);
    return 0;
}
