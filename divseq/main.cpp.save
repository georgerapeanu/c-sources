#include <cstdio>
#include <vector>
using namespace std;
FILE *f=fopen("divseq.in","r");
FILE *g=fopen("divseq.out","w");
long long i,j;
long long rez,N,fst,fdr;
int V[250001];
int val[250001];
void shft(long long poz)
{
    for(long long k=j-i+1;k>poz;k--)
    {
        V[k]=V[k-1];
    }
}
bool ok()
{
    bool ey=0;
    long long st=0,mid=0,last=0;
    long long dr=j-i;
    while(st<=dr)
    {
        mid=(st+dr)/2;
        if(V[mid]>val[j])
            dr=mid-1;
        else
        {
            st=mid+1;
            last=mid;
        }
    }
    if((V[last]==0||val[j]%V[last]==0)&&V[last+1]%val[j]==0)
    {
        ey=1;
    }
    shft(last+1);
    V[last+1]=val[j];
    return ey;
}
void sterge()
{
    long long st=1,mid=0,last=0;
    long long dr=j-i+1;
    while(st<=dr)
    {
        mid=(st+dr)/2;
        if(V[mid]>val[i])
            dr=mid-1;
        else
        {
            st=mid+1;
            last=mid;
        }
    }
    for(long long k=last;k<=j-i;k++)
        V[k]=V[k+1];
}
int main()
{
    fscanf(f,"%d",&N);
    for(i=1;i<=N;i++)
    {
        fscanf(f,"%d",&val[i]);
    }
    i=j=1;
    fst=fdr=0;
    while(j<N)
    {
        while(j<N&&ok())
        {
            j++;
        }
        rez=rez+(j-i)*(j-i+1)/2-(j-fst)*(j-fst+1);
        fst=i;
        fdr=j;
        while(!ok())
        {
            sterge();
            i++;
        }
        j++;
    }
    fprintf(g,"%lld",rez);
    return 0;
}
