#include<cstdio>
#include<cstring>
using namespace std;
FILE *f=fopen("cezar.in","r");
FILE *g=fopen("cezar.out","w");
char c[300];
int k[11],j,N,i;
int main()
{
    fgets(c+1,300,f);
    c[0]='.';
    for(i=1;i<=10;i++)
        fscanf(f,"%d",&k[i]);
    N=strlen(c);
        for(i=1;i<N;i++)
        {
            if(c[i]!=' '&&c[i]!='\n')
            {
                j++;
                c[i]=(c[i]-k[(j-1)%10+1]-'A')%26+'A';
                
            }
        }
    fputs(c+1,g);
    fclose(f);
    fclose(g);
     return 0;
}
