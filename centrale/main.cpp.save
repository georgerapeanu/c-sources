#include <fstream>
using namespace std;
ifstream f("centrale.in");
ofstream g("centrale.out");
char c,nr;
bool adev;
int nr,i,n;
int main()
{
    f>>n
    for(i=1;i<=n;i++)
    {
        f>>c;
        f>>c;
        nr=c;
        adev=1;
        while(c>='0'&&c<='9')
        {
            f>>c;
            if(c!=nr)
            {
                f>>c;
                if(c>='0'&&c<='9')
                {
                    f.putback(c);
                    adev=0;
                }
            }
        }
        nr+=(adev==true)?(1:0);
    }
    g<<nr;
    f.close();
    g.close()
    return 0;
}
