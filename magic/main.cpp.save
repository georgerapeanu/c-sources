#include <cstdio>
using namespace std;
FILE *f=fopen("magic.in","r");
FILE *g=fopen("magic.out","w");
int N,P;
long long C,nr;
int main()
{
    fscanf(f,"%lld %d %d",&C,&N,&P);
    while(N)
    {
        if(N%2==1)
        {nr*=C;nr%=P;}
        C=C*C;
        C%=P;
        N/=2;
    }
    
    return 0;
}
