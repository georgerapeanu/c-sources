#include <cstdio>
using namespace std;
FILE *f=fopen("ron.in","r");
FILE *g=fopen("ron.out","w");
int k,nrc,nrl,nrs;
int main()
{
    fscanf(f,"%d\n%d %d",&k,&nrl,&nrs);
    while(k>1)
    {
        nrc++;
        k/=2;
    }
    if(nrl!=nrc&&nrs!=nrc)
        fprintf(g,"O");
    
    fclose(f);
    fclose(g);
    return 0;
}
