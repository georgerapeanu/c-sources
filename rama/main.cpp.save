#include <cstdio>
using namespace std;
FILE *f=fopen("rama.in","r");
FILE *g=fopen("rama.out","w");
int i,j,N,M,m[705][705],coloane[705],maxim,k,dist,E[705][705],unde[705];
char V[705][705];
int main()
{
    fscanf(f,"%d %d\n",&N,&M);
    for(i=1;i<=N;i++)
    {
        for(j=1;j<=M;j++)
        {
            fscanf(f,"%c",&V[i][j]);
            if(V[i][j]-'0')
                m[i][j]=m[i-1][j]+1;
        }
        fscanf(f,"\n");
    }
    for(i=1;i<=N;i++)
    {
        for(j=M;j>=1;j--)
            if(V[i][j]-'0')
                E[i][j]=E[i][j+1]+1;
    }
    for(i=1;i<=N;i++)
    {
        for(j=i;j<=N;j++)
        {
                coloane[0]=0;
            for(k=1;k<=M;k++)
            {
                if(m[j][k]>=j-i+1)
                {
                    coloane[++coloane[0]]=k;
                }
            }
            unde[coloane[0]]=coloane[coloane[0]];
            dist=0;///verifici daca coloane e emtpy
            for(k=coloane[0]-1;k>0;k--)
            {
                if(coloane[k]+E[i][k]>=coloane[k+1]&&coloane[k]+E[j][k]>=coloane[k+1])
                    unde[k]=unde[k+1];
                else
                    unde[k]=coloane[k];
                if(unde[k]-coloane[k]+1>dist)
                    dist=unde[k]-coloane[k]+1;
            }
            if((j-i+1)*dist>maxim)
                maxim=(j-i+1)*dist;

        }
    }
    fprintf(g,"%d",maxim);
    fclose(f);
    fclose(g);
    return 0;
}
