#include <iostream>
#include <cstring>
using namespace std;
ifstream f("spioni.in");
ofstream g("spioni.out");
char v[4096];
void program(int low, int high, char vec[])
{
    if(low<high)
    {
    int i=0,j=0,med=(low+high)/2;
    char v1[2048]={'\0'},v2[2048]={'\0'};
    for(i=0;i<=high-low+1;i+=2)
    {v1[j]=vec[i];v2[j++]=vec[i+1];}
    program(low,med,v1);
    program(med+1,high,v2);
    }
    else
    {
        v[low]=vec[0];
    }
}
